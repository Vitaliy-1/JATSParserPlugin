msgstr "JATSParser plugin settings menu"

msgid "plugins.generic.jatsParser.settings.covert.pdf"
msgstr "Activate JATS XML to PDF conversion"

msgid "plugins.generic.jatsParser.galley.import"
msgstr "Import of JATS XML galleys"

msgid "plugins.generic.jatsParser.galley.import.description"
msgstr "This options allows to save JATS XML galleys as a full-text across all publications in the journal. Import is omitted if a publication already has full-text for a given galley. Import starts immediately after saving the form"

msgid "plugins.generic.jatsParser.galley.import.title"
msgstr "Import full-text from galleys"

msgid "plugins.generic.jatsParser.publication.jats.fulltext"
msgstr "Full Text"

msgid "plugins.generic.jatsParser.publication.jats.fulltext.success"
msgstr "Full-text upload successful"

msgid "plugins.generic.jatsParser.publication.jats.label"
msgstr "Create full text"

msgid "plugins.generic.jatsParser.publication.jats.description"
msgstr "Select a production ready file in JATS XML format to create the full text."

msgid "plugins.generic.jatsParser.publication.jats.descriptionEmpty"
msgstr "The list of JATS XML files is empty. Please, upload the file to the production stage and it will appear below for full-text generation."

msgid "plugins.generic.jatsParser.publication.jats.references.label"
msgstr "Retrieve references from JATS XML?"

msgid "plugins.generic.jatsParser.publication.jats.references.description"
msgstr "Reference list will be retrieved from JATS XML and saved as a part of the publication. If references are already included into the publication, they are going to be rewritten."

